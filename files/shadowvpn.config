
config minivtun
	option enable '0'
	option server '127.0.0.1'
	option port '1414'
	option password 'my_password'
	option net '10.7.0.2/24'
	option mtu '1300'
	option keepalive '13'
	option intf 'mv0'
#	route_mode 0: all ip via VPN
#	route_mode 1: exclude chnroute.txt via VPN
#	route_mode 2: include chnroute.txt via VPN
	option route_mode '1'
	option route_file '/etc/chinadns_chnroute.txt'
